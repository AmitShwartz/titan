{
  "extends": "@tsconfig/node16/tsconfig.json",
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./",
    "outDir": "dist"
  },
  "exclude": ["node_modules", "dist"],
  "include": ["src", "src/models/User.js"]
}
